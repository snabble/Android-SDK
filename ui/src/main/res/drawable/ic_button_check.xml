<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="2dp" />
            <size
                android:height="36dp"
                android:width="36dp" />
            <solid android:color="@color/snabble_primaryColor" />
            <stroke
                android:color="@color/snabble_primaryColorDark"
                android:width="0.75dp" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_check_white"
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp" />
</layer-list>