<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
            xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="4dp" />
            <size
                android:height="40dp"
                android:width="40dp" />
            <solid android:color="@color/snabble_primaryColor" />
            <stroke
                android:color="@color/snabble_primaryColorDark"
                android:width="1dp" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_check_white"
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />
</layer-list>